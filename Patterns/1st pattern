----code----
class Main {
    public static void main(String[] args) {
        int n=5;
        int start=5;
        int row=1;
        while(row<=n){
            int i=1;
            while(i<=start){
                System.out.print("*");
                i++;
            }
            System.out.println();
            row++;
        }
    }
}
